#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>

#define ALPHA 0
#define SYMB 1
#define FUNC 2
#define F_KEYS 3


/ {
    combos {
        compatible = "zmk,combos";
        
        // ESC → Q + W
        combo_esc {
            timeout-ms = <50>;
            layers = <ALPHA>;
            key-positions = <0 1>;
            bindings = <&kp ESC>;
        };

        // TAB → A + S
        combo_tab {
            timeout-ms = <50>;
            layers = <ALPHA>;
            key-positions = <10 11>;
            bindings = <&kp TAB>;
        };

        // ENTER → J + K + L
        combo_enter {
            timeout-ms = <50>;
            layers = <ALPHA>;
            key-positions = <16 17 18>;
            bindings = <&kp ENTER>;
        };

        // Х[ru] → O + P
        combo_ha {
            timeout-ms = <50>;
            layers = <ALPHA>;
            key-positions = <8 9>;
            bindings = <&kp LEFT_BRACKET>;
        };

        // CAPS WORD → F + J
        combo_caps_word {
            timeout-ms = <50>;
            layers = <ALPHA>;
            key-positions = <14 17>;
            bindings = <&caps_word>;
        };
    };

    
    behaviors {
        // ----- HOME ROW MODIFIERS -----
        // Left hand home row mods with positional hold-tap
        hml: home_row_modifier_left {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            require-prior-idle-ms = <150>;
            tapping-term-ms = <300>;
            quick-tap-ms = <175>;
            bindings = <&kp>, <&kp>;
            hold-trigger-key-positions = <5 6 7 8 9 10 15 16 17 18 19 20 25 26 27 28 29 30>;
            hold-trigger-on-release;
        };
        
        // Standard home row mod
        hrm: home_row_modifier {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <300>;
            quick-tap-ms = <0>;
            bindings = <&kp>, <&kp>;
        };
    };

    
    keymap {
        compatible = "zmk,keymap";

            // → - hold tap
            // » - double tap
            // ➤ - shifted

            // Home row modifiers →
            // +------------------------------------------------------------------+
            // |  Q  |  W  |  E  |  R  |  T  |     |  Y  |  U  |  I  |  O  |  P  |
            // | CTR | ALT | CMD | SHF |  G  |     |  H  | SHF | CMD | ALT | CTR |
            // |  Z  |  X  |  C  |  V  |  B  |     |  N  |  M  |  ,  |  .  |  /  |
            // +------------------------------------------------------------------+
            
            // Combos
            // +------------------------------------------------------------------+
            // |  Q     W  |  E  |  R  |  T  |     |  Y  |  U  |  I  |  O  |  P  |
            // |--- Esc ---|                                         |-- Х[ru] --|
            // |  A     S  |  D  |  F  |  G  |     |  H  |  J  |  K  |  L  |  ;  |
            // |--- Tab ---|                             |----- Enter -----|
            // |                    ┗       CAPS WORD       ┛
            // |  Z  |  X  |  C  |  V  |  B  |     |  N  |  M  |  ,  |  .  |  /  |
            // +------------------------------------------------------------------+

